<div class="col-md-4">
  <div class="thumbnail">
    <%= link_to image_tag(photo.picture.thumb.url), photo %>
    <div class="caption">
      <h3><%= photo.title %></h3>
      <p>
        tags
      </p>
      <p>
        <%= link_to 'Edit', edit_photo_path(photo) %>
        <%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %>
      </p>
    </div>
  </div>
</div>